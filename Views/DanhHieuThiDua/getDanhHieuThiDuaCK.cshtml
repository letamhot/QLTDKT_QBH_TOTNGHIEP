                                document.getElementById("btnUpdate").disabled = true;
                            }


                        }
                    }
                }
                else if ($("#bophan").val() == 3) {
                    if ($("#loaiDH").val() == 1) {
                        if (data == "True") {
                            $("#txtTenDanhHieuThiDua").text("");
                            document.getElementById("btnUpdate").disabled = false;
                        }
                        else {
                            if ($("#tenDanhHieuThiDua").val() == "") {
                                $("#txtTenDanhHieuThiDua").text("");
