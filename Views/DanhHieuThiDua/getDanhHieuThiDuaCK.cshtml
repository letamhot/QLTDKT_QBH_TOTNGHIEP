                    }
                    else if ($("#loaiDH").val() == 0) {
                        if (data == "True") {
                            $("#txtTenDanhHieuThiDua").text("");
                            document.getElementById("btnUpdate").disabled = false;
                        }
                        else {
                            if ($("#tenDanhHieuThiDua").val() == "") {
                                $("#txtTenDanhHieuThiDua").text("");
                                document.getElementById("btnUpdate").disabled = false;
                            }
                            else {

                                $("#txtTenDanhHieuThiDua").text("Tên danh hiệu thi đua đã trùng");

