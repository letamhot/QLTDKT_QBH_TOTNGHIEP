<script>
    var flag = 0;
    $(document).ready(function () {
        activeMenu("liDanhMuc", "liDanhHieuThiDua", "liDmDanhHieuCK");
        $.noConflict();
        validator();
        loadData();
        loadChuKy();
        loadCapKyKT();
